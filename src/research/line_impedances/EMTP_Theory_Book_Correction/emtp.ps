/rejust { (e) search
 { /pre exch def pop /post exch def
   /lpre pre length def /lpost post length def
   0 1 6 {
     pre lpre 1 sub get 48 eq pre lpre 2 sub get 46 ne and % ?.*0 -> ?.*
       { /pre pre 0 lpre 1 sub getinterval def /lpre pre length def } if
   } for
   pre (1) eq { /pre () def /lpre 0 def } if
   pre (-1) eq { /pre (-) def /lpre 1 def } if
   post 0 get 48 eq /fixz0 load {
     post 1 get 48 eq post 0 get 45 eq and /fixz0m load
     { post 1 get 48 eq post 0 get 43 eq and /fixz0p load if } ifelse
   } ifelse
   currentpoint /:y0 exch def /:x0 exch def
   lpre 0 ne pre (-) ne and {
     pre addlen
     SYMlarge sf
     (´) addlen
     HBlarge sf
     (10) addlen
     post addlen
     pre s (´) syms (10) s post sups }
     {  pre addlen (10) addlen
     post addlen
     pre s (10) s post sups } ifelse
 }
 { dup (0.010) eq { pop (0.01) } if rjust } ifelse } bd
